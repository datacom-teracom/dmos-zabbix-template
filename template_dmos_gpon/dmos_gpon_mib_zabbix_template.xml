        <template>
            <uuid>5bfff143d325461cad250ec5a0db8659</uuid>
            <template>DM Template - DmOS GPON</template>
            <name>DM Template - DmOS GPON</name>
            <groups>
                <group>
                    <name>DM - Templates</name>
                </group>
                <group>
                    <name>DmOS</name>
                </group>
                <group>
                    <name>Templates</name>
