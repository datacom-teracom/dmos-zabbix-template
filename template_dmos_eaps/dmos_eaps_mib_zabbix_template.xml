<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>4.4</version>
    <date>2022-12-20T12:36:00Z</date>
    <groups>
        <group>
            <name>DM - Templates</name>
        </group>
        <group>
            <name>Templates/Network devices</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>DM Template - DMOS-EAPS-MIB</template>
            <name>DM Template - DMOS-EAPS-MIB</name>
            <groups>
                <group>
                    <name>DM - Templates</name>
                </group>
                <group>
                    <name>Templates/Network devices</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>EAPS</name>
                </application>
            </applications>
            <discovery_rules>
                <discovery_rule>
                    <name>EAPS Domains</name>
                    <type>SNMPV2</type>
                    <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                    <snmp_oid>discovery[{#EAPS_ID},DMOS-EAPS::eapsDomainID,{#EAPS_NAME},DMOS-EAPS::eapsDomainName]</snmp_oid>
                    <key>snmp.discovery.eaps.domains</key>
                    <delay>60m</delay>
                    <description>Discovering EAPS domains from DMOS-EAPS-MIB.</description>
                    <item_prototypes>
                        <item_prototype>
                            <name>EAPS  {#EAPS_ID} Healt Check State</name>
                            <type>SNMPV2</type>
                            <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                            <snmp_oid>DMOS-EAPS::eapsDomainHealthCheckState.{#SNMPINDEX}</snmp_oid>
                            <key>eapsDomainHealthCheckState.[{#SNMPINDEX}]</key>
                            <delay>5m</delay>
                            <applications>
                                <application>
                                    <name>EAPS</name>
                                </application>
                            </applications>
                            <valuemap>
                                <name>DmOS-EAPS-MIB Healt Check State</name>
                            </valuemap>
                        </item_prototype>
                        <item_prototype>
                            <name>EAPS  {#EAPS_ID} Domain ID</name>
                            <type>SNMPV2</type>
                            <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                            <snmp_oid>DMOS-EAPS::eapsDomainID.{#SNMPINDEX}</snmp_oid>
                            <key>eapsDomainID.[{#SNMPINDEX}]</key>
                            <delay>5m</delay>
                            <applications>
                                <application>
                                    <name>EAPS</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>EAPS  {#EAPS_ID} Domain Mode</name>
                            <type>SNMPV2</type>
                            <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                            <snmp_oid>DMOS-EAPS::eapsDomainMode.{#SNMPINDEX}</snmp_oid>
                            <key>eapsDomainMode.[{#SNMPINDEX}]</key>
                            <delay>5m</delay>
                            <applications>
                                <application>
                                    <name>EAPS</name>
                                </application>
                            </applications>
                            <valuemap>
                                <name>DmOS-EAPS-MIB Domain Mode</name>
                            </valuemap>
                        </item_prototype>
                        <item_prototype>
                            <name>EAPS {#EAPS_ID} Domain Name</name>
                            <type>SNMPV2</type>
                            <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                            <snmp_oid>DMOS-EAPS::eapsDomainName.{#SNMPINDEX}</snmp_oid>
                            <key>eapsDomainName.[{#SNMPINDEX}]</key>
                            <delay>5m</delay>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <applications>
                                <application>
                                    <name>EAPS</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>EAPS  {#EAPS_ID} Domain Primary Port State</name>
                            <type>SNMPV2</type>
                            <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                            <snmp_oid>DMOS-EAPS::eapsDomainPrimaryPortState.{#SNMPINDEX}</snmp_oid>
                            <key>eapsDomainPrimaryPortState.[{#SNMPINDEX}]</key>
                            <delay>5m</delay>
                            <applications>
                                <application>
                                    <name>EAPS</name>
                                </application>
                            </applications>
                            <valuemap>
                                <name>DmOS-EAPS-MIB Port State</name>
                            </valuemap>
                        </item_prototype>
                        <item_prototype>
                            <name>EAPS {#EAPS_ID} Domain Secondary Port State</name>
                            <type>SNMPV2</type>
                            <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                            <snmp_oid>DMOS-EAPS::eapsDomainSecondaryPortState.{#SNMPINDEX}</snmp_oid>
                            <key>eapsDomainSecondaryPortState.[{#SNMPINDEX}]</key>
                            <delay>5m</delay>
                            <applications>
                                <application>
                                    <name>EAPS</name>
                                </application>
                            </applications>
                            <valuemap>
                                <name>DmOS-EAPS-MIB Port State</name>
                            </valuemap>
                        </item_prototype>
                        <item_prototype>
                            <name>EAPS  {#EAPS_ID} Domain State</name>
                            <type>SNMPV2</type>
                            <snmp_community>{$SNMP_COMMUNITY}</snmp_community>
                            <snmp_oid>DMOS-EAPS::eapsDomainState.{#SNMPINDEX}</snmp_oid>
                            <key>eapsDomainState.[{#SNMPINDEX}]</key>
                            <delay>5m</delay>
                            <applications>
                                <application>
                                    <name>EAPS</name>
                                </application>
                            </applications>
                            <valuemap>
                                <name>DmOS-EAPS-MIB Domain State</name>
                            </valuemap>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{last()}&lt;&gt;2</expression>
                                    <name>EAPS {#EAPS_ID} ({#EAPS_NAME}) domain is not complete</name>
                                    <priority>HIGH</priority>
                                    <description>Alarm if EAPS domain state is different from Complete.</description>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
            </discovery_rules>
        </template>
    </templates>
    <value_maps>
        <value_map>
            <name>DmOS-EAPS-MIB Domain Mode</name>
            <mappings>
                <mapping>
                    <value>0</value>
                    <newvalue>Master</newvalue>
                </mapping>
                <mapping>
                    <value>1</value>
                    <newvalue>Transit</newvalue>
                </mapping>
            </mappings>
        </value_map>
        <value_map>
            <name>DmOS-EAPS-MIB Domain State</name>
            <mappings>
                <mapping>
                    <value>0</value>
                    <newvalue>Idle</newvalue>
                </mapping>
                <mapping>
                    <value>1</value>
                    <newvalue>Init</newvalue>
                </mapping>
                <mapping>
                    <value>2</value>
                    <newvalue>Complete</newvalue>
                </mapping>
                <mapping>
                    <value>3</value>
                    <newvalue>Failed</newvalue>
                </mapping>
                <mapping>
                    <value>4</value>
                    <newvalue>Pre-forwarding</newvalue>
                </mapping>
                <mapping>
                    <value>5</value>
                    <newvalue>Links-up</newvalue>
                </mapping>
                <mapping>
                    <value>6</value>
                    <newvalue>Link-down</newvalue>
                </mapping>
            </mappings>
        </value_map>
        <value_map>
            <name>DmOS-EAPS-MIB Healt Check State</name>
            <mappings>
                <mapping>
                    <value>0</value>
                    <newvalue>ok</newvalue>
                </mapping>
                <mapping>
                    <value>1</value>
                    <newvalue>failed</newvalue>
                </mapping>
            </mappings>
        </value_map>
        <value_map>
            <name>DmOS-EAPS-MIB Port State</name>
            <mappings>
                <mapping>
                    <value>0</value>
                    <newvalue>up-enabled</newvalue>
                </mapping>
                <mapping>
                    <value>1</value>
                    <newvalue>up-blocked</newvalue>
                </mapping>
                <mapping>
                    <value>2</value>
                    <newvalue>down-enabled</newvalue>
                </mapping>
                <mapping>
                    <value>3</value>
                    <newvalue>down-blocked</newvalue>
                </mapping>
            </mappings>
        </value_map>
    </value_maps>
</zabbix_export>
